@model SJOne.Models.JudgeViewModels.RaceResoultsViewModel

@{
    ViewBag.Title = "RaceResoult";
}

<h2>RaceResoult</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th></th>
        <th></th>
        <th></th>
    </tr>

@foreach (var item in Model.HandTimings) {
    <tr>
        <td>
            @item.StartNumber.Number
        </td>
        <td>
            @item.StartNumber.User.Name @item.StartNumber.User.Surname
        </td>
        <td>
           @item.TotalTime.Value.ToString()
        </td>

    </tr>
}

</table>
