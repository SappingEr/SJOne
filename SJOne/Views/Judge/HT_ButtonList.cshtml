@model SJOne.Models.ButtonListViewModel

<div class="row">
    <div class="col"></div>
    <div class="col-md-8">
        @foreach (var item in Model.HandTimings)
        {
            var number = item.StartNumber.Number;
            <button id="@number" type="button" class="btn-lg btn-primary ml-2 mb-2">
                @number
                <span class="badge badge-light">
                    @item.Lap
                </span>
            </button>
        }
    </div>
    <div class="col"></div>
</div>





